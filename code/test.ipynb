from TFlight import SignalAnalyzer,calculate_green_occ

analyzer = SignalAnalyzer(
    file_path="spat_3_20250915000000_20250916000000.txt",
    target_phase_ids=[11, 29, 30, 46, 50, 51],
    begin_time="2025-09-15 05:00:00",
    end_time="2025-09-15 12:00:00"
)

results = analyzer.run()

for key, seq in results.items():
    print(f"组 {key} 匹配 {len(seq)} 条记录")


greenocc, csv_rows = calculate_green_occ(results, output_path="record/green_ratio_cycles_3.csv")


